<Window x:Class="client_V1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:client_V1"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="115*"/>
            <ColumnDefinition Width="403*"/>
        </Grid.ColumnDefinitions>
        <Button x:Name="register" Content="Register" HorizontalAlignment="Left" Height="50" Margin="30,80,0,0" VerticalAlignment="Top" Width="130" Grid.ColumnSpan="2" Click="register_Click"/>
        <Button x:Name="login" Content="Login" Grid.Column="1" HorizontalAlignment="Left" Height="50" Margin="79.8,80,0,0" VerticalAlignment="Top" Width="110" Click="login_Click"/>
    </Grid>
</Window>
